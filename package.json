{
  "name": "cypress-test-report",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "e2e": "cypress run --record false --reporter mocha-multi-reporters --reporter-options configFile=cypress.json ",
    "merge-results": "mochawesome-merge --reportDir cypress/results > mochawesome.json",
    "generate-report": "npx mochawesome-report-generator mochawesome.json",
    "test": "npm-run-all -s delete:reports e2e merge-results generate-report",
    "delete:reports": "rm cypress/results/* || true"
  },
  "keywords": [],
  "author": "",
  "description": "",
  "devDependencies": {
    "cypress": "^3.3.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.23.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.0.0",
    "mochawesome-merge": "^2.0.0",
    "mochawesome-report-generator": "^4.0.0",
    "npm-run-all": "^4.1.5"
  }
}
